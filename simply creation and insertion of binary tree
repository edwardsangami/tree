/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
    Node root;
    
    Codechef(){
        root=null;
    }
    Codechef(int data){
        root=new Node(data);
    }
	public static void main (String[] args) throws java.lang.Exception
	{
	
	BufferedReader bf = new BufferedReader(new InputStreamReader(System.in));
	Codechef cf=new Codechef();
	cf.root=new Node(1);
    cf.root.left=new Node(2);
    cf.root.left.left=new Node(4);
    cf.root.right=new Node(3);
    System.out.println(cf.root.right.key);
	
	
	}
}
class Node{
    
    int key;
    Node left,right;
    
    public Node(int data){
        key=data;
        left=right=null;
    }
}
